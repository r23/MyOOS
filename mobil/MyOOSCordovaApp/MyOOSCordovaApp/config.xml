<?xml version="1.0" encoding="utf-8"?>
<widget id="com.coolappz.MyOOSCordovaApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>MyOOSCordovaApp</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.php" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <vs:toolsetVersion xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">6.3.1</vs:toolsetVersion>
  <engine name="android" spec="~5.2.1" />
  <engine name="ios" spec="4.2.0" />
  <engine name="windows" spec="4.4.2" />
  <plugin name="cordova-plugin-camera" spec="~2.4.1" />
  <plugin name="cordova-plugin-contacts" spec="~2.3.1" />
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
  <plugin name="cordova-plugin-compat" spec="~1.1.0" />
  <preference name="Fullscreen" value="True" />
  <plugin name="cordova-plugin-file" spec="~4.3.3" />
  <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
  <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
  <preference name="AndroidLaunchMode" value="standard" />
</widget>