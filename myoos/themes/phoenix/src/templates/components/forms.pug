extends ../partials/layout-components

//- Override default config variables
append config
  - var pageTitle = 'Forms'
  - var pageSubTitle = "&mdash;&nbsp; Form control styles, layout options, and custom components. <a href='https://getbootstrap.com/docs/4.3/components/forms/' target='_blank'>See Bootstrap documentation</a>."
  - var isNestedFile = true
  - var isCodeHighlighting = true
  - var breadcrumbs = [{'title': 'Home', 'link': '../index.html'}, {'title': 'Components', 'link': '#'}]

//- Page Content
block content

  // Supported input types
  h2.h5.mb-3 Supported input types
  .card
    .card-header
      ul(role='tablist').nav.nav-tabs.card-header-tabs
        li.nav-item
          a(href='#result1' data-toggle='tab' role='tab' aria-controls='result1' aria-selected='true').nav-link.active Result
        li.nav-item
          a(href='#html1' data-toggle='tab' role='tab' aria-controls='html1' aria-selected='false').nav-link HTML
        li.nav-item
          a(href='#pug1' data-toggle='tab' role='tab' aria-controls='pug1' aria-selected='false').nav-link Pug
    .card-body
      .tab-content
        #result1.tab-pane.fade.show.active(role='tabpanel')
          .form-group.row.align-items-center
            label(for='text-input').col-md-2.col-form-label Text
            .col-md-10
              input(type='text' id='text-input' value='Artisanal kale').form-control
          .form-group.row.align-items-center
            label(for='search-input').col-md-2.col-form-label Search
            .col-md-10
              input(type='search' id='search-input' value='How do I shoot web').form-control
          .form-group.row.align-items-center
            label(for='email-input').col-md-2.col-form-label Email
            .col-md-10
              input(type='email' id='email-input' value='email@example.com').form-control
          .form-group.row.align-items-center
            label(for='url-input').col-md-2.col-form-label URL
            .col-md-10
              input(type='url' id='url-input' value='https://getbootstrap.com').form-control
          .form-group.row.align-items-center
            label(for='tel-input').col-md-2.col-form-label Phone
            .col-md-10
              input(type='tel' id='tel-input' value='1-(770)-334-2518').form-control
          .form-group.row.align-items-center
            label(for='pass-input').col-md-2.col-form-label Password
            .col-md-10
              input(type='password' id='pass-input' value='mypasswordexample').form-control
          .form-group.row
            label(for='textarea-input').col-md-2.col-form-label Textarea
            .col-md-10
              textarea(id='textarea-input' rows='5').form-control Hello World!
          .form-group.row.align-items-center
            label(for='select-input').col-md-2.col-form-label Select
            .col-md-10
              select(id='select-input').form-control.custom-select
                option Choose option...
                option Option item 1
                option Option item 2
                option Option item 3
          .form-group.row.align-items-center
            label(for='multiselect-input').col-md-2.col-form-label Multiselect
            .col-md-10
              select(id='multiselect-input' size='3' multiple).form-control
                option Option item 1
                option Option item 2
                option Option item 3
                option Option item 4
                option Option item 5
                option Option item 6
          .form-group.row.align-items-center
            label(for='file-input').col-md-2.col-form-label File
            .col-md-10
              .custom-file
                input(type='file' id='file-input').custom-file-input
                label(for='file-input').custom-file-label Choose file...
          .form-group.row.align-items-center
            label(for='number-input').col-md-2.col-form-label Number
            .col-md-10
              input(type='number' id='number-input' value='37').form-control
          .form-group.row.align-items-center
            label(for='date-time-input').col-md-2.col-form-label Date and time
            .col-md-10
              input(type='datetime-local' id='date-time-input').form-control
          .form-group.row.align-items-center
            label(for='date-input').col-md-2.col-form-label Date
            .col-md-10
              input(type='date' id='date-input' value='2018-06-13').form-control
          .form-group.row.align-items-center
            label(for='month-input').col-md-2.col-form-label Month
            .col-md-10
              input(type='month' id='month-input' value='2018-06').form-control
          .form-group.row.align-items-center
            label(for='week-input').col-md-2.col-form-label Week
            .col-md-10
              input(type='week' id='week-input' value='2018-W33').form-control
          .form-group.row.align-items-center
            label(for='time-input').col-md-2.col-form-label Time
            .col-md-10
              input(type='time' id='time-input' value='14:45:00').form-control
          .form-group.row.align-items-center
            label(for='color-input').col-md-2.col-form-label Color
